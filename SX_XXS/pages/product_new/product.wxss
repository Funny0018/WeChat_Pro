/* pages/product/product.wxss */

.search {
  height: 50rpx;
  margin-top: 10rpx;
  outline: none;
  border: none;
  border-radius: 0.8rem;
  padding-left: 1.4rem;
  padding-right: 0.5rem;
  color: #fff;
  background: #ff944d;
  background-size: 0.8rem;
  font-size: 30rpx;
  width: 250rpx;
  display: inline-block;
}

.address {
  padding-left: 20rpx;
  font-size: 30rpx;
  height: 50rpx;
  line-height: 50rpx;
  width: 400rpx;
  display: inline-block;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.searchImg {
  width: 1.2rem;
  height: 1.2rem;
  display: inline-block;
  padding-bottom: 0.2rem;
  padding-left: 0.3rem;
}

.product-top {
  background-color: #f60;
  /* white-space: nowrap; */
  height: 100rpx;
  line-height: 80rpx;
  width: 100%;
  position: fixed;
  top: 0;
  z-index: 10;
}

.productList {
  /* background-color: #f60; */
  background-color: #fff;
  margin-top: 10rpx;
  width: 100%;
  /* height: 30rem; */
}

.productItem {
  background-color: #fff;
  /* border-bottom: #f60 solid 0.1rem; */
  height: 300rpx;
  width: 100%;
}

.productDetail {
  height: 300rpx;
  width: 100%;
}

.productImgView {
  float: left;
  height: 100%;
  width: 30%;
}

.productImg {
  width: 260rpx;
  height: 260rpx;
  padding-left: 20rpx;
  padding-top: 20rpx;
}

.productInfoView {
  float: left;
  margin-left: 80rpx;
  width: 30%;
  height: 250rpx;
  margin-top: 20rpx;
}

.productName {
  width: 450rpx;
  margin-top: 0.5rem;
  font-size: 30rpx;
  font-weight: 800;
  margin-left: 10rpx;
  float: left;
  position: absolute;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.productDesc {
  width: 450rpx;
  margin-top: 1.8rem;
  font-size: 30rpx;
  margin-left: 10rpx;
  float: left;
  position: absolute;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.priceView {
  width: 200rpx;
  float: left;
  font-size: 30rpx;
  margin-top: 130rpx;
  margin-left: 10rpx;
}

.priceViewnovip {
  width: 200rpx;
  float: left;
  font-size: 30rpx;
  margin-left: 10rpx;
  margin-top: 170rpx;
}

.productPrice {
  font-size: 30rpx;
  color: #f60;
}

.productVipPrice {
  color: #ffd700;
  font-size: 35rpx;
}

.vipimage {
  width: 25rpx;
  height: 25rpx;
  margin-left: 20rpx;
}

.vippriceimage {
  width: 80rpx;
  height: 35rpx;
  margin-left: 10rpx;
  margin-bottom: -5rpx;
}

.dian-cart {
  width: 25%;
  height: 50rpx;
  margin-right: 0;
  position: absolute;
  right: -20rpx;
  margin-top: 200rpx;
}

.dian-cart image {
  width: 70rpx;
  height: 70rpx;
  float: right;
  margin-right: 45rpx;
  margin-top: -10rpx;
}

.dian {
  width: 25%;
  height: 50rpx;
  margin-right: 0;
  position: absolute;
  right: -10rpx;
  margin-top: 120rpx;
}

.stepper {
  display: inline;
}

.stepper text {
  float: left;
  height: 50rpx;
  width: 40rpx;
  text-align: center;
  font-size: 50rpx;
  color: #f60;
  margin-left: 10rpx;
  margin-top: 80rpx;
}

.stepper input {
  float: left;
  height: 50rpx;
  width: 50rpx;
  text-align: center;
  font-size: 40rpx;
  margin-top: 90rpx;
}

.login-info {
  background-color: rgba(0, 0, 0, 0.5);
  width: 100%;
  height: 100%;
  position: fixed;
  z-index: 399;
}

.login {
  background-color: #ddd;
  position: fixed;
  width: 50%;
  height: 50%;
  margin-top: 25%;
  margin-left: 25%;
  z-index: 400;
}

.loginbut {
  background-color: #fff;
  margin-top: 50%;
}

.type-list {
  height: 80rpx;
  margin-top: 70rpx;
  z-index: 10;
}

.type {
  height: 70rpx;
  line-height: 70rpx;
  width: 160rpx;
  font-size: 30rpx;
  display: inline-block; /*  这个也很重要 */
  text-align: center;
  box-sizing: border-box;
}

.active {
  /* border-bottom: 2rpx solid #f60; */
  font-weight: 700;
  font-size: 35rpx;
}

.active::after {
  content: '';
  position: absolute;
  display: block;
  margin-top: 0;
  margin-left: 7.5%;
  height: 1px;
  width: 6%;
  background-color: #f60;
}

.scroll-header {
  height: 80rpx;
  display: flex;
  white-space: nowrap; /* 这个很重要， */
  z-index: 999;
  background-color: #fff;
  position: fixed;
}

.good_box {
  width: 18px;
  height: 18px;
  position: fixed;
  border-radius: 50%;
  overflow: hidden;
  left: 50%;
  top: 50%;
  z-index: +99;
  /* background: rgba(0, 133, 207, 1); */
  background-color: #f60;
  /* background-image: url('/static/image/product/cart.png'); */
}

.isBottom {
  margin-top: 20rpx;
  font-size: 30rpx;
  color: #ddd;
  text-align: center;
  padding-bottom: 20rpx;
}

.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background-color: rgba(0, 0, 0, 0.5);
  overflow: hidden;
  z-index: 9999;
  display: flex;
  align-items: center;
  justify-content: center;
}

.modal-dialog {
  width: 540rpx;
  overflow: hidden;
  z-index: 9999;
  background: #f9f9f9;
  border-radius: 5rpx;
}

.modal-title {
  padding-top: 30rpx;
  font-size: 32rpx;
  color: #030303;
  text-align: center;
}

.modal-content {
  padding: 20rpx 32rpx;
  font-size: 28rpx;
}

.modal-footer {
  display: flex;
  flex-direction: row;
  height: 86rpx;
  border-top: 1px solid #dedede;
  font-size: 34rpx;
  line-height: 86rpx;
}

.btn-cancel {
  width: 50%;
  color: #abb4bd;
  text-align: center;
  border-right: 1px solid #dedede;
}

.btn-confirm {
  width: 50%;
  color: #6fb64b;
  text-align: center;
  font-weight: 500;
}

